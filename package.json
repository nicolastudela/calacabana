{
  "name": "calacabana",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "export": "next build && next export",
    "prisma:db:push": "dotenv -e .env.local -- npx prisma db push",
    "prisma:db:seed": "dotenv -e .env.local -- npx prisma db seed",
    "prisma:db:migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:db:migrate:reset": "dotenv -e .env.local -- npx prisma migrate reset",
    "prisma:db:migrate:deploy": "dotenv -e .env.local -- npx prisma migrate deploy"
  },
  "prisma": {
    "seed": "ts-node --project ./prisma/tsconfig.json ./prisma/seed.ts"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^2.4.7",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/montecarlo": "^4.5.10",
    "@fontsource/montserrat": "^4.5.13",
    "@next/bundle-analyzer": "^12.1.5",
    "@prisma/client": "^4.9.0",
    "@sendgrid/mail": "^7.7.0",
    "date-fns": "^2.28.0",
    "framer-motion": "^8.4.1",
    "googleapis": "^100.0.0",
    "is-mobile": "^3.1.1",
    "next": "^13.1.1",
    "next-connect": "^0.12.2",
    "nuka-carousel": "^5.0.16",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.0",
    "react-icons": "^4.3.1",
    "sharp": "^0.30.4",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "17.0.24",
    "@types/react": "18.0.5",
    "@types/react-datepicker": "^4.4.2",
    "@types/react-dom": "18.0.1",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.5",
    "prisma": "^4.9.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "4.6.3"
  }
}
